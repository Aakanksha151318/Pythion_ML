def Factorial():
  num = int(input("Enter Number here:-"))
  factorial = 1
  if num < 0:
    print("Enter number is Negative")
  elif num == 0:
    print("The factorial of 0 is 1")
  else:
    for i in range(1, num + 1):
      factorial = factorial * i
    print("The factorial of", num, "is", factorial)

def main():
  Factorial()
if __name__ =="__main__":
  main()
